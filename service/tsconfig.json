{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true
    },
  "exclude": [
    "node_modules",
    "node_modules_private",
    "Scripts",
    "*.d.ts",
    "util/preload/templates/*"
  ],
    "compileOnSave": true
}
